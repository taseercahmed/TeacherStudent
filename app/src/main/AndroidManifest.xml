<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.teacherstudent">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
       >

        <activity android:name=".TeacherResult"></activity>
        <activity android:name=".Student.StudentResult" />
        <activity android:name=".Feechallan" />
        <activity
            android:name=".ImageActivity"
            android:theme="@style/ToolbarTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Student.StudentMainActivity" />
        </activity>
        <activity
            android:name=".TeacherclassesActivity"
            android:theme="@style/ToolbarTheme">
